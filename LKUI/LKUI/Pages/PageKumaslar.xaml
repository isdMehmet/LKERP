    <Grid>
        <toolkit:ChildWindow Name="ChildBilgi" IsModal="True" Height="Auto" WindowStartupLocation="Center">
            <dtl:DtlKumasTipBilgileri x:Name="DtlKumasBilgi" Kaydedildi="DtlKumasBilgi_Kaydedildi" />
        </toolkit:ChildWindow>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="231*" />
                <ColumnDefinition Width="74*" />
                <ColumnDefinition Width="128*" />
                <ColumnDefinition Width="508*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="7*" />
                <RowDefinition Height="34*" />
                <RowDefinition Height="15*" />
                <RowDefinition Height="504*" />
            </Grid.RowDefinitions>
            
            <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.RowSpan="2" Visibility="Hidden">
                <Button Content="Ekle" Width="70"  Grid.Row="1"  Name="BtnEkle" Click="BtnEkle_Click" />
                <Button Content="Düzenle" Width="70"  Grid.Row="1"  Name="BtnDüzelt" Click="BtnDüzelt_Click" />
                <Button Content="Sil" Width="70"  Grid.Row="1"  Name="BtnSil" Click="BtnSil_Click" />
            </StackPanel>
            <DataGrid x:Name="DGridKumaslar" Grid.ColumnSpan="4" Grid.Row="3" AutoGenerateColumns="False" MouseDoubleClick="DGridKumaslar_MouseDoubleClick" IsReadOnly="True">
                <DataGrid.Columns>
                    <DataGridTextColumn  Header="Tip No" MinWidth="150" IsReadOnly="True" Binding="{Binding Path=TipNo}" />
                    <DataGridTextColumn Header="Tip Adi" Width="300" IsReadOnly="True" Binding="{Binding Path=TipAdi}" />
                    <DataGridTextColumn  Header="Varyant" Width="1*" IsReadOnly="True" Binding="{Binding Path=TipNo}" />
                </DataGrid.Columns>
            </DataGrid>
            <StackPanel Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="5" Grid.RowSpan="2" Orientation="Horizontal" Height="30" Margin="20,0,0,0">
                <toolkit:WatermarkTextBox Watermark="No'ya Göre Ara" Name="TxtNo" Width="200" TextChanged="TxtNo_TextChanged"/>
                <toolkit:WatermarkTextBox Watermark="Ada Göre Ara"  Name="TxtAdi" Width="200" Margin="20, 0,0,0" TextChanged="TxtAdi_TextChanged"/>
            </StackPanel>

        </Grid>
